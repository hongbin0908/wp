class Chapt_6_2_001 {
  def totalResultOverRange(number: Int, codeBlock: Int => Int):Int = {
    var result = 0
    for (i <- 1 to number) {
      result += codeBlock(i)
    }
    result
  }
  
  def main(args: Array[String]) = {
    val instance = new Chapt_6_2_00
    
  }
  
}

object Sample {
  def main(args: Array[String]) = println("Hello Scala")
